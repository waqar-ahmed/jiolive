############################
# gitignore for android
############################

# Built application files
*.apk
*.aar
*.ap_
*.aab

# Files for the ART/Dalvik VM
*.dex

# Java class files
*.class

# Generated files
app/bin/
app/gen/
app/out/
#  Uncomment the following line in case you need and you don't have the release build type files in your app
# release/

# Gradle files
app/.gradle/
app/build/

# Local configuration file (sdk path, etc)
app/local.properties

# Proguard folder generated by Eclipse
app/proguard/

# Log Files
*.log

# Android Studio Navigation editor temp files
app/.navigation/

# Android Studio captures folder
app/captures/

# IntelliJ
*.iml
*/.idea/workspace.xml
*/.idea/tasks.xml
*/.idea/gradle.xml
*/.idea/assetWizardSettings.xml
*/.idea/dictionaries
*/.idea/libraries
*/.idea
# Android Studio 3 in .gitignore file.
app/.idea/caches
app/.idea/modules.xml
# Comment next line if keeping position of elements in Navigation Editor is relevant for you
app/.idea/navEditor.xml

# Keystore files
# Uncomment the following lines if you do not want to check your keystore files in.
#*.jks
#*.keystore

# External native build folder generated in Android Studio 2.2 and later
app/.externalNativeBuild
app/.cxx/

# Google Services (e.g. APIs or Firebase)
# google-services.json

# Freeline
app/freeline.py
app/freeline/
app/freeline_project_description.json

# fastlane
app/fastlane/report.xml
app/fastlane/Preview.html
app/fastlane/screenshots
app/fastlane/test_output
app/fastlane/readme.md

# Version control
app/vcs.xml

# lint
app/lint/intermediates/
app/lint/generated/
app/lint/outputs/
app/lint/tmp/
# lint/reports/

# Android Profiling
app/*.hprof
app/app/google-services.json

app/app/.cxx/cmake/

google-services.json

cloudnest.keystore

keystore_content.sh

app/nativetemplates/build/intermediates/incremental/packageDebugResources/

app/nativetemplates/build/intermediates/merged_java_res/debug/
